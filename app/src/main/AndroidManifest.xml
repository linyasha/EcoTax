<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.linyashik.ecotax">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EcoTax">
        <activity android:name=".app.activity.loginSignup.register.Register3rdClass"/>
        <activity android:name=".app.activity.loginSignup.register.Register2ndClass"/>
        <activity android:name=".app.activity.loginSignup.register.Register"/>
        <activity android:name=".app.activity.loginSignup.Login"/>
        <activity android:name=".app.activity.OnBoarding"/>
        <activity android:name=".app.activity.DashBoard" />
        <activity android:name=".app.activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>